<%= options.middleware.map(path =>`import ${'middleware_' + hash(path)} from '${path.replace(/\\/g,'/')}';`).join('\n') %>

export default (ctx) => {
<%= options.middleware.map(path =>`\t\tmiddleware_${hash(path)}(ctx);`).join('\n') %>
};
